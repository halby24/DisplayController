{
    "cmake.buildDirectory": "${workspaceFolder}/build",
    "cmake.configureOnOpen": true,
    "cmake.configureArgs": [
        "-DCMAKE_BUILD_TYPE=Debug"
    ],
    "cmake.generator": "Visual Studio 17 2022",
    "cmake.preferredGenerators": [
        "Visual Studio 17 2022"
    ],
    "cmake.outputLogEncoding": "utf8",
    "cmake.environment": {
        "VSLANG": "1033"
    },
    "files.associations": {
        "*.uxml": "xml",
        "*.uss": "css",
        "ssh_config": "ssh_config",
        ".env.*": "properties",
        "*.lilblock": "shaderlab",
        "*.env": "properties",
        "CmakeSettings.txt": "cmake",
        "*.i": "swig",
        ".secrets": "properties",
        ".secrets.example": "properties",
        ".vars": "properties",
        ".vars.sample": "properties",
        "filesystem": "cpp"
    }
}
